{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/utils",
    "packages/graph",
    "packages/contracts",
    "packages/client"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "externalPattern": [
    "./packages/contracts/src/typechain-types/**",
    "./packages/graph/src/generated/**",
    "./packages/utils/src/math/bigDecimal.ts",
    "node_modules/**"
  ],
  "excludeExternals": true,
  "out": "apps/docs",
  "plugin": [
    "typedoc-plugin-missing-exports"
  ]
}