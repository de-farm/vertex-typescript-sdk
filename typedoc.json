{
  "$schema": "https://typedoc.org/schema.json",
  "commentStyle": "all",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/utils",
    "packages/graph",
    "packages/contracts",
    "packages/client"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "externalPattern": [
    "node_modules/**"
  ],
  "out": "apps/docs",
  "internalModule": "Internal",
  "sort": [
    "visibility",
    "source-order"
  ],
  "plugin": [
    "typedoc-plugin-missing-exports"
  ]
}